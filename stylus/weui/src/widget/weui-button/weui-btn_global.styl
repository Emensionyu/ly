@import"../../base/fn"

.weui-btn
    position relative
    display block
    margin-left: auto
    margin-left auto 
    line-height 2.5556
    padding-left  14px
    padding-right  14px
    box-sizing border-box
    font-size weuiBtnFontSize 
    text-align center
    text-decoration: none
    color weuiBtnfontColor
    border-radius: weuiBtnBorderRadius 
    setTapColor()
    overflow hidden

    .weui-btn_inline
        display inline-block

